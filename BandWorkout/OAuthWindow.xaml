<Window x:Class="BandWorkout.OAuthWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BandWorkout"
        mc:Ignorable="d"
        Title="OAuthWindow" Height="600" Width="400" Loaded="Window_Loaded">
    <Grid>
        <WebBrowser x:Name="webBrowser" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Navigated="webBrowser_Navigated" />
    </Grid>
</Window>
